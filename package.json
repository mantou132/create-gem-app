  "keywords": [
    "gem",
    "boilerplate"
  ],
  "author": "mt",
  "license": "ISC",
  "dependencies": {
    "colors": "^1.4.0",
    "commander": "^6.2.0",
    "lodash": "^4.17.20",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.6",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  },
  "husky": {
