{
  "name": "create-gem-app",
  "version": "0.0.9",
  "description": "quickly create gem app",
  "bin": "build/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -watch",
    "lint": "eslint --ext .ts --fix src/",
    "test": "jest --coverage",
    "prepare": "npm run lint && npm run test && npm run build"
  },
  "files": [
    "/index.js",
    "/index.js.map",
    "/index.d.ts",
    "/index.d.ts.map",
    "/build/",
    "/src/"
